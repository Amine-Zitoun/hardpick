<div class="main">
    <div class="main-header"> 
        <div class="icon-container">
            <canvas id="icon" width="100" height="100"></canvas>
        </div>
    </div>
    <div class="content">
        <div class="general-info">
            <div class="wlc">Welcome to ALPHAPICKER</div>
            <div class="explain">Select what is below and you are set</div>
        </div>
        <form action="/search" method="POST">
                <div class="data-inputs">
                    <div class="detail">
                        <div class="inp-title"><label>Component</label></div>
                        <div class="menu-selc">
                            <select name="comp">
                                <option>CPU</option>
                                <option>GPU</option>
                                <option>MOTHERBOARD</option>
                            </select>
                        </div>
                    </div>
                    <div class="detail-mid">
                        <div class="inp-title"><label>Region</label></div>
                        <div class="inp-item">
                            <select name="reg">
                                <option>Tunisia</option>
                            </select>
                        </div>
                    </div> 
                    <div class="detail">
                        <div class="inp-title"><label>Price</label></div>
                        <div class="inp-item">
                            <input type="number" name="price" min="100">
                        </div>
                    </div>    
                    <div class="btn-row">
                        <button class="btn-primary" type="submit">Search</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>